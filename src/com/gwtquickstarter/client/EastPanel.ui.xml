<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:widgets='urn:import:com.gwtquickstarter.client.widgets'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>

        <div style="top: 10px; right: 35px;">
            <span style="font-size: 10pt; float: right;">
                <widgets:LoginStatusWidget/>
            </span>
        </div>


        <div id="feedback_div" class="blueVerticalGradient rounded">
            <widgets:ClickableLabel ui:field="feedbackLink" text="Feedback"/>
        </div>

        <div>
            <g:VerticalPanel ui:field="vpanel"/>
        </div>

    </g:HTMLPanel>

</ui:UiBinder>
